{
  "openapi": "3.0.0",
  "info": {
    "title": "API Ticketera",
    "version": "1.0.0",
    "description": "API documentada con OpenAPI y Redoc"
  },
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "paths": {
    "/auth": {
      "get": {
        "summary": "Autenticación del usuario",
        "security": [
          {
            "cookieAuth": []
          }
        ],
        "tags": [
          "Auth"
        ],
        "responses": {
          "200": {
            "description": "Realiza la autenticación del usuario",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "email": {
                        "type": "string"
                      },
                      "password": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {},
  "tags": [
    {
      "name": "Auth",
      "description": "Operaciones de autenticación"
    }
  ]
}